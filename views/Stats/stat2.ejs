        <!--style="width:900px; height:800px; overflow:auto;"-->
        <div id="tableauViz1" ></div>
        <script>
            function initializeViz() {
                // JS object that points at empty div in the html
                var placeholderDiv = document.getElementById("tableauViz1");
                // URL of the viz to be embedded
                var url = "https://public.tableau.com/views/First100goals/Dashboard1";
                // An object that contains options specifying how to embed the viz
                //https://public.tableau.com/profile/ellenn#!/vizhome/WorldIndicators/GDPpercapita
                //https://public.tableau.com/profile/shridhar8226/#!/vizhome/FootBallAnalysis_15731554533090/Sheet1
                var options = {

                    hideTabs: true,
                    hideToolbar: true,
                };
                viz = new tableau.Viz(placeholderDiv, url, options);
                viz.setFrameSize(800,800)
            }
            initializeViz();
        </script>
        </div>

            
